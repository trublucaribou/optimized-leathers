<?xml version="1.0" encoding="utf-8" ?>

<Patch>

<!-- Change Vanilla Animals Expanded - Arid Shrubland -->

<Operation Class="PatchOperationFindMod">
    <mods>
        <li>VanillaExpanded.VAEAS</li>
    </mods>
    <match Class="PatchOperationReplace">
        <xpath>/Defs/ThingDef[defName = "AEXP_Coyote"]/race/leatherDef</xpath>
      <value>
         <leatherDef>Leather_ShortFur</leatherDef>
      </value>
    </match>
</Operation>

